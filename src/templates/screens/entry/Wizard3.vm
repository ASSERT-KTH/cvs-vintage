#set ( $action = $link.setPage("entry/Wizard3.vm").setAction("ReportIssue").addPathInfo("nextTemplate", "entry/Wizard4.vm") )
#set ($user = $scarabR.User)
#set ($module = $user.CurrentModule)
#set ($issue = $user.ReportingIssue)


  <FORM METHOD="get" NAME="Report3Form" action="$action">
  <TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="100%">
    <TR>
      <TD>

      </TD>
    </TR>
    <TR>
      <TD HEIGHT="1" BGCOLOR="#000000"><SPACER TYPE="block" HEIGHT="1"></TD>
    </TR>
</TABLE>
<P>
<!-- current project and login -->
  <TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="100%">
    <TR>
      <TD BGCOLOR="#FFFFFF" HEIGHT="1"><SPACER HEIGHT="1" TYPE="block"></TD>
      <TD ALIGN="right" NOWRAP CLASS="PlainText">Logged in: <A HREF="/">jon 123</A></TD>
    </TR>
  </TABLE>
<!-- end current project and login -->

<!-- enter info -->
  <TABLE BORDER="0" CELLPADDING="2" CELLSPACING="0" WIDTH="100%">
    <TR>
      <TD NOWRAP CLASS="Header" BGCOLOR="#999999">REPORT AN ISSUE: STEP 3</TD>
    </TR>
    <TR>
      <TD CLASS="SectionHeader" BGCOLOR="#CCCCCC">Current Issue Information
</TD>
    </TR>
  </TABLE>

  <TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="100%">
 <tr>
  <td valign="top" class="TableDataHeader">Project:</td>
  <td valign="top" class="TableData">Scarab</td>
  <td valign="top">&nbsp;</td>
  <td valign="top" class="TableDataHeader">Component:</td>
  <td valign="top" class="TableData">Viewer</td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
 <tr>
  <td height="1" colspan="5" bgcolor="#cccccc"><spacer type="block" height="1"></td>
 </tr>
 <tr>
  <td valign="top" class="TableDataHeader">Type:</td>
  <td valign="top" class="TableData">Feature</td>
  <td valign="top">&nbsp;</td>
  <td valign="top" class="TableDataHeader">Version:</td>
  <td valign="top" class="TableData">Beta1</td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
 <tr>
  <td height="1" colspan="5" bgcolor="#cccccc"><spacer type="block" height="1"></td>
 </tr>
 <tr>
  <td valign="top" class="TableDataHeader">Platform:</td>
  <td valign="top" class="TableData">

          #attrValueErrorMsg ( $issue "PLATFORM" )
          #attrValueSelect ($issue "PLATFORM")


  </td>
  <td valign="top">&nbsp;</td>
  <td valign="top" class="TableDataHeader">OS:</td>
  <td valign="top" class="TableData">
          #attrValueErrorMsg ( $issue "OPERATING SYSTEM" )
          #attrValueSelect ($issue "OPERATING SYSTEM")
  </td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
 <tr>
  <td height="1" colspan="5" bgcolor="#cccccc"><spacer type="block" height="1"></td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
 <tr>
  <td valign="top" class="TableDataHeader">Summary:</td>
  <td valign="top" colspan="4" class="TableData">

    #attrValueErrorMsg( $issue "SUMMARY" )<br>
    #set ( $attrValue = $issue.AllAttributeValuesMap.get("SUMMARY") )
    #set ( $attrInput = $intake.AttributeValue.mapTo($attrValue) ) 
   <input type="hidden" name="$attrInput.CID" value="$attrInput.OID">
   <input name= "$attrInput.Value.Key"
        value="$!attrInput.Value.Value" size="20" type="text">

</td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
 <tr>
  <td height="1" colspan="5" bgcolor="#cccccc"><spacer type="block" height="1"></td>
 </tr>
 <tr>
  <td height="2" colspan="5"><spacer type="block" height="2"></td>
 </tr>
  </TABLE>
<!-- end enter info-->
<p>
<!-- enter info -->
  <TABLE BORDER="0" CELLPADDING="2" CELLSPACING="0" WIDTH="100%">
    <TR>
      <TD COLSPAN="6" WIDTH="100%" CLASS="SectionHeader" BGCOLOR="#CCCCCC">Enter Issue Information</TD>
    </TR>
    <TR>
      <TD ALIGN="right" VALIGN="top">
        <SPAN CLASS="SectionHeader">URL:
      </TD>
      <TD COLSPAN="3">

    #set ( $attrValue = $issue.ModuleAttributeValuesMap.get("URL") )
    #set ( $attrInput = $intake.AttributeValue.mapTo($attrValue) ) 
    #if ( !$attrInput.Url.isValid() )
        $attrInput.Url.Message<br>
    #end
   <input type="hidden" name="$attrInput.CID" value="$attrInput.OID">
   <input name= "$attrInput.Url.Key"
        value="$!attrInput.Url.Value" size="30" type="text">

      </TD>
    </TR>
    <TR>
      <TD ALIGN="right" VALIGN="top">
        <SPAN CLASS="SectionHeader">Description:
      </TD>
      <TD COLSPAN="3">

    #attrValueErrorMsg( $issue "DESCRIPTION" )<br>
    #set ( $attrValue = $issue.ModuleAttributeValuesMap.get("DESCRIPTION") )
    #set ( $attrInput = $intake.AttributeValue.mapTo($attrValue) ) 
   <input type="hidden" name="$attrInput.CID" value="$attrInput.OID">
   <textarea name= "$attrInput.Value.Key" cols="40" 
           rows="5">$!attrInput.Value.Value</textarea>

      </TD>
    </TR>
    <TR>
      <TD>&nbsp;</TD>
      <TD>
            <input type="submit" 
                name="eventSubmit_doEnterissue" value="Submit Issue">
        </FORM>
      </TD>
    </TR>
    <TR>
      <TD COLSPAN="6">
        <TABLE BORDER="0" CELLPADDING="0" CELLSPACING="0" WIDTH="100%">
          <TR><TD BGCOLOR="#CCCCCC" HEIGHT="1"><SPACER
HEIGHT="1" TYPE="block"></TD></TR></TABLE>
      </TD>
    </TR>
  </TABLE> 
  </FORM> 

## ARTIFACT TYPES
#set ($issueTypes = $scarabR.CurrentModule.NavIssueTypes)
#if ($issueTypes.size() > 0)

<div><small><strong><a href="$link.setPage("SelectArtifactType.vm")">Artifact types</a></strong></small>
#foreach($issueType in $issueTypes)
  #if ($issueType.equals($scarabR.CurrentIssueType))
    <div><small><strong>$issueType.Name</strong></small>
    <div><small>#if ($scarabR.CurrentModule.Dedupe)<a href="$link.setPage("entry,Wizard1.vm")">#else<a href="$link.setPage("entry,Wizard3.vm")">#end Enter new</a></small></div>
    <div><small><a href="$link.setPage("AdvancedQuery.vm")">Advanced Query</a></small></div>
    <div><small><a href="$link.setPage("Search.vm")">Custom Query</a></small></div>
    <div><small><a href="$link.setPage("QueryList.vm")">Saved queries</a></small></div>
    <div><small><a href="$link.setPage("TemplateList.vm")">Template list</a></small></div>
    <div><small><a href="$link.setPage("reports,Step1.vm")">New report</a></small></div>
    <div><small>Saved reports</small></div></div>
  #else
    <div><small><a href="$link.setPage("$scarabR.convertPath($data.Target)").setAction("SelectIssueType").addPathInfo("nextTemplate", $scarabR.convertPath($data.Target)).addPathInfo("newissuetype", "$issueType.IssueTypeId")">$issueType.Name</a></small></div>
  #end
#end
</div>

#else
  #if ($data.User.hasLoggedIn())
    <a href="$link.setPage("SelectModule.vm")"><strong>Select Module</strong></a>
  #else
    <a href="$link.setPage("Login.vm")"><strong>Login</strong></a>
  #end
#end

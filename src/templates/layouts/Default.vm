<!DOCTYPE html PUBLIC "-//Tigris//DTD XHTML 1.0 Transitional//EN"
    "http://style.tigris.org/nonav/tigris_transitional.dtd">
#set ($page.Title = $l10n.Title)
<html>
 <head>
  <title>$page.Title</title>
  <meta http-equiv="Content-type" content="$data.ContentType" />
  <style type="text/css">
  @import url($staticLink.setPath("/style/tigris.css"));
  </style>
  <link rel="stylesheet" type="text/css" href="$staticLink.setPath("/style/print.css")" media="print" />
<script src="$staticLink.setPath("/scripts/main.js")" type="text/javascript">
if (document.layers) { document.writeln('<link rel="stylesheet" type="text/css" href="$staticLink.setPath("/style/ns4_only.css")" media="screen" />')
}</script>
 </head>
<body marginwidth="0" marginheight="0" class="composite">
 <div id="banner">
  <table border="0" cellspacing="0" cellpadding="8" width="100%">
  <tr>
  <td>
  <a href="$link.setPage("Index.vm")"><img src="$staticLink.setPath("/images/logo.gif")" alt="Scarab" width="118" height="23" border="0" /></a>
  </td>
  <td>
  <div align="right" id="login">
   #if ($data.User.hasLoggedIn())
   <div>
    Logged in: <strong class="username">$!data.User.FirstName $!data.User.LastName</strong>
   </div>
   #end
   <div>
    #if ($data.User.hasLoggedIn())
    $link.setPage("Login.vm").setAction("Logout").setLabel("Logout")
    #else
    $link.setPage("Login.vm").setLabel("Login")
    #end
   </div>
  </div>
  </td>
  </tr>
  </table>
 </div>

<div id="breadcrumbs"><table border="0" cellspacing="0" cellpadding="4" width="100%"><tr><td> 
#if ($scarabR.CurrentModule)
 <strong>Current module:</strong>
 $link.setPage("SelectModule.vm").setLabel("$scarabR.CurrentModule.Name")
#else
 &nbsp;
#end
</td></tr></table></div>


<table border="0" cellspacing="0" cellpadding="8" width="100%" id="main">
 <tr valign="top">
  <td id="leftcol" width="20%">
  <div id="navcolumn">
$renderer.render("navigations", $template, "Default.vm")
  </div>
<div id="helptext">
<table border="0" cellspacing="0" cellpadding="3" width="100%">
<tr>
<th>How do I...?</th>
</tr>
<tr>
<td>
  $renderer.render("navigations", "Help.vm")
</td>
</tr>
</table>
</div>
 </td>
 <td><div id="bodycol">
 <!-- Scarab - App -->
$renderer.render("layouts", "MainContent.vm")
<!-- Structural Elements -->
</div>
 </td>
#if ($template.equals("SelectModule.vm"))
  $renderer.render("layouts", "/ModuleHelp.vm")
#elseif ($template.equals("SelectArtifactType.vm"))
  $renderer.render("layouts", "/ArtifactTypeHelp.vm")
#end
 </tr>
</table>
<!-- Structural Elements -->
$renderer.render("navigations", "/ScarabBottom.vm")
</body>
</html>

<!DOCTYPE html PUBLIC "-//CollabNet//DTD XHTML 1.0 Transitional//EN"
    "http://collabnet.com/dtds/collabnet_transitional_10.dtd">
<html>
 <head>
  <title>Scarab</title>
<!--  <meta http-equiv="Content-type" content="text/html; charset=ISO-8859-1" /> -->
<!--  <meta http-equiv="Content-type" content="text/html; charset=EUC-KR" /> -->
  <meta http-equiv="Content-type" content="$data.ContentType" />

  <style type="text/css">
  @import url($data.Request.ContextPath/style/ns4_toxins.css);
  </style>

  <link rel="stylesheet" type="text/css" href="$data.Request.ContextPath/style/main.css" />
  <link rel="stylesheet" type="text/css" href="$data.Request.ContextPath/style/print.css" media="print" />
  <link rel="alternate stylesheet" title="compact" type="text/css" href="$data.Request.ContextPath/style/compact.css" />
 </head>
<body marginwidth="0" marginheight="0">
 <div id="banner">
  <table border="0" cellspacing="0" cellpadding="8" width="100%">
  <tr>
  <td>
  <a href="$link.setPage("Index.vm")"><img src="$data.Request.ContextPath/images/logo.gif" alt="Scarab" width="118" height="23" border="0" /></a>
  </td>
  <td>
  <div align="right" id="login">
      #if ($data.User.FirstName)
   <div>
    Logged in: <strong class="username">$data.User.FirstName $data.User.LastName</strong>
   </div>
            #end
   <div>
#if ($scarabR.CurrentModule)
   Current Module:
   <a href="$link.setPage("SelectModule.vm")"><strong>$scarabR.CurrentModule.Name</strong></a>
#else
   <a href="$link.setPage("SelectModule.vm")"><strong>Select Module</strong></a>
#end
   </div>
   <div>
    #if ($data.User.hasLoggedIn())
    <a href="$link.setPage("Login.vm").setAction("Logout")">Logout</a>
    #else
    <a href="$link.setPage("Login.vm")">Login</a>
    #end
  | <a href="$link.setPage("entry,Wizard1.vm")">Enter Artifact</a>
   </div>
  </div>
  </td>
  </tr>
  </table>
 </div>

 <table border="0" cellspacing="0" cellpadding="8" width="98%" id="main">
 <tr valign="top">
  <td id="leftcol" width="20%">
  <div id="navcolumn">
$renderer.render("navigations", $data, $template, "Default.vm")
  </div>
  <div id="helptext">
   <table border="0" cellspacing="0" cellpadding="3" width="100%">
   <tr>
   <th>How do I...?</th>
   </tr>
   <tr>
   <td>
     <div>Learn more about this project? </div> <div>Print this page...</div>
     <div>Troubleshoot...</div>
   </td>
   </tr>
   </table>
 </div>
 </td>
 <td width="100%" id="bodycol">
 <!-- Scarab - App -->
 <div id="topmodule">
  <table border="0" cellspacing="0" cellpadding="3" width="100%">
  <tr>
   <td nowrap="nowrap">
  <div id="navsearch">
$renderer.render("navigations", $data, $template, "SearchNav.vm")
  </div>
   </td>
  </tr>
  </table>
 </div>
 <!-- Scarab - App -->
#actionErrorMsg()
$renderer.render("screens", $data, $template)
<!-- Structural Elements -->
 </td>
 </tr>
 </table>
<!-- Structural Elements -->
$renderer.render("navigations", $data, "/ScarabBottom.vm")
 </body>
</html>

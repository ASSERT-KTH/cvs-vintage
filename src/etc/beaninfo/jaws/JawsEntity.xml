<bean 
  class="org.jboss.ejb.plugins.jaws.deployment.JawsEntity"
  displayname="Entity"
  iconcolor16="/images/bean.gif">
  
  <property 
    name="DisplayName" class="java.lang.String" expert="true"
    displayname="Display name" readonly="true"/>
    
  <property 
    name="EjbName" class="java.lang.String" 
    displayname="EJB name" readonly="true"/>
    
  <property 
    name="TableName" class="java.lang.String" 
    displayname="Table name"/>
    
  <property 
    name="CreateTable" class="boolean" 
    displayname="Create on deploy" propertyeditor="com.dreambean.awt.editors.BooleanEditor"/>

  <property 
    name="RemoveTable" class="boolean" 
    displayname="Remove on un-deploy" propertyeditor="com.dreambean.awt.editors.BooleanEditor"/>
    
  <property 
    name="TunedUpdates" class="boolean" 
    displayname="Tuned updates" propertyeditor="com.dreambean.awt.editors.BooleanEditor"/>

  <property 
    name="ReadOnly" class="boolean" 
    displayname="Read only" propertyeditor="com.dreambean.awt.editors.BooleanEditor"/>
	 
  <property 
    name="TimeOut" class="int" 
    displayname="Timeout"/>
	 
  <method name="addFinder" displayname="Add finder">
  </method>
  
</bean>

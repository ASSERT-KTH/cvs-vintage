<bean 
  class="org.jboss.ejb.deployment.RandomEntityInstanceCacheConfiguration"
  displayname="Random-passivating cache"
  iconcolor16="/images/database.gif"
  stopclass="org.jboss.ejb.deployment.NoPassivationEntityInstanceCacheConfiguration">
  
  <property 
    name="MinimumSize" class="int" 
    displayname="Minimum size"/>
    
  <property 
    name="MaximumSize" class="int" 
    displayname="Maximum size"/>
    
  <property 
    name="PassivationInterval" class="int" 
    displayname="Passivation interval (seconds)"/>
</bean>
   

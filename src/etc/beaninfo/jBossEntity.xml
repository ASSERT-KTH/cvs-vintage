<bean 
  class="org.jboss.ejb.deployment.jBossEntity"
  displayname="Entity"
  iconcolor16="/images/bean.gif">
  
  <property 
    name="DisplayName" class="java.lang.String" expert="true"
    displayname="Display name" readonly="true"/>
    
  <property 
    name="EjbName" class="java.lang.String" 
    displayname="EJB name" readonly="true"/>
    
  <property 
    name="PersistenceType" class="java.lang.String" expert="true"
    displayname="Persistence type" readonly="true"/>
    
  <property 
    name="JndiName" class="java.lang.String" 
    displayname="JNDI name"/>
    
  <property 
    name="ConfigurationName" class="java.lang.String" 
    displayname="Container configuration"/>
</bean>

	    <sql-type>TIMESTAMP</sql-type>
	 </mapping>
	 <mapping>
	    <java-type>java.sql.Date</java-type>
	    <jdbc-type>DATE</jdbc-type>
	    <sql-type>DATE</sql-type>
	 </mapping>
	 <mapping>
	    <java-type>java.sql.Time</java-type>
	    <jdbc-type>TIME</jdbc-type>

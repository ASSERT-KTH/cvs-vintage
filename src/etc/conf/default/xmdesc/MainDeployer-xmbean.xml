<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mbean PUBLIC
   "-//JBoss//DTD JBOSS XMBEAN 1.1//EN"
   "http://www.jboss.org/j2ee/dtd/jboss_xmbean_1_1.dtd">

<!-- $Id: MainDeployer-xmbean.xml,v 1.1 2004/09/02 02:36:34 patriot1burke Exp $ -->
<mbean>
	 <description>MainDeployer</description>
	 <class>org.jboss.deployment.MainDeployer</class>
	 &defaultAttributes;
   <attribute access="read-write" getMethod="getSuffixOrder" setMethod="setSuffixOrder">
      <description>Get the ordering of the deployment suffixes</description>
      <name>SuffixOrder</name>
      <type>java.lang.String[]</type>
      <descriptors>
         <default value="deployer,sar,rar,ds.xml,service.xml,har,jar,war,wsr,ear,zip,last"/> 
      </descriptors>
   </attribute>
	 <attribute access="read-write" getMethod="getCopyFiles" setMethod="setCopyFiles">
	 	<description>Get the flag indicating whether directory content will be deployed</description>
	 	<name>CopyFiles</name>
	 	<type>boolean</type>
	 </attribute>
   <attribute access="read-write" getMethod="getTempDir" setMethod="setTempDir">
      <description>Get the temp directory</description>
      <name>TempDir</name>
      <type>java.io.File</type>
   </attribute>
   <attribute access="read-only" getMethod="getTempDirString">
      <description>Get the temp directory</description>
      <name>TempDirString</name>
      <type>java.lang.String</type>
   </attribute>
   <attribute access="write-only" setMethod="setServiceController">
      <description>Get the temp directory</description>
      <name>ServiceController</name>
      <type>javax.management.ObjectName</type>
   </attribute>
	 <operation>
		 <name>listDeployed</name>
		 <return-type>java.util.Collection</return-type>
	 </operation>
   <operation>
      <name>listDeployedModules</name>
      <return-type>java.util.Collection</return-type>
   </operation>
   <operation>
      <name>listDeployedAsString</name>
      <return-type>java.lang.String</return-type>
   </operation>
   <operation>
      <name>listIncompletelyDeployed</name>
      <return-type>java.util.Collection</return-type>
   </operation>
   <operation>
      <name>listWaitingForDeployer</name>
      <return-type>java.util.Collection</return-type>
   </operation>
   <operation>
      <name>addDeployer</name>
      <parameter>
         <name>deployer</name>
         <type>org.jboss.deployment.SubDeployer</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>removeDeployer</name>
      <parameter>
         <name>deployer</name>
         <type>org.jboss.deployment.SubDeployer</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>listDeployers</name>
      <return-type>java.util.Collection</return-type>
   </operation>
   <operation>
      <name>shutdown</name>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>checkIncompleteDeployments</name>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>redeploy</name>
      <parameter>
         <name>urlspec</name>
         <type>java.lang.String</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>redeploy</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>redeploy</name>
      <parameter>
         <name>sdi</name>
         <type>org.jboss.deployment.DeploymentInfo</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>deploy</name>
      <parameter>
         <name>urlspec</name>
         <type>java.lang.String</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>deploy</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>deploy</name>
      <parameter>
         <name>sdi</name>
         <type>org.jboss.deployment.DeploymentInfo</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>undeploy</name>
      <parameter>
         <name>urlspec</name>
         <type>java.lang.String</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>undeploy</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>undeploy</name>
      <parameter>
         <name>sdi</name>
         <type>org.jboss.deployment.DeploymentInfo</type>
      </parameter>
      <return-type>void</return-type>
   </operation>
   <operation>
      <name>isDeployed</name>
      <parameter>
         <name>url</name>
         <type>java.lang.String</type>
      </parameter>
      <return-type>boolean</return-type>
   </operation>
   <operation>
      <name>isDeployed</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>boolean</return-type>
   </operation>
   <operation>
      <name>getDeployment</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>org.jboss.deployment.DeploymentInfo</return-type>
   </operation>
   <operation>
      <name>getWatchUrl</name>
      <parameter>
         <name>url</name>
         <type>java.net.URL</type>
      </parameter>
      <return-type>java.net.URL</return-type>
   </operation>
	 &defaultOperations;
 </mbean>